config_version=5

[application]

config/name="networking_jam"
run/main_scene="res://scenes/levels/title.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"DRAGON": "res://dialogue/DRAGON.dch",
"Griffin": "res://assets/griffin/Griffin.dch",
"player": "res://assets/player/player.dch",
"wingsworth": "res://assets/griffin/wingsworth.dch"
}
directories/dtl_directory={
"ending dialogue": "res://dialogue/ending dialogue.dtl",
"first dialogue with griffin": "res://dialogue/first dialogue with griffin.dtl",
"second_star": "res://dialogue/second_star.dtl",
"third timeline": "res://dialogue/third timeline.dtl"
}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/style_list=["res://assets/dialogic_stye.tres"]
layout/default_style="res://assets/dialogic_stye.tres"
glossary/default_case_sensitive=true
variables={}

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true

